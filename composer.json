{
    "name": "ectouch/ectouch",
    "type": "project",
    "description": "A Modern E-Commerce Platform.",
    "keywords": [
        "ectouch",
        "framework"
    ],
    "homepage": "https://www.ectouch.cn",
    "license": "GPL-3.0",
    "support": {
        "issues": "https://github.com/ectouch/ectouch/issues?state=open",
        "forum": "https://www.ectouch.cn/forum/",
        "wiki": "https://www.ectouch.cn/wiki/",
        "source": "https://github.com/ectouch/ectouch"
    },
    "require": {
        "php": "^7.1.3",
        "aliyuncs/oss-sdk-php": "^2.3",
        "endroid/qr-code": "^3.5",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^5.0",
        "jialeo/payment": "^0.9.1",
        "nesbot/carbon": "^2.11",
        "overtrue/easy-sms": "^1.1",
        "overtrue/pinyin": "^4.0",
        "overtrue/wechat": "^4.1",
        "phpoffice/phpspreadsheet": "^1.6",
        "ramsey/uuid": "^3.8",
        "topthink/framework": "5.1.*",
        "topthink/think-captcha": "^2.0",
        "topthink/think-image": "^1.0",
        "topthink/think-migration": "^2.0",
        "topthink/think-queue": "^2.0",
        "topthink/think-worker": "^2.0",
        "workerman/phpsocket.io": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "think-path": "bootstrap"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
